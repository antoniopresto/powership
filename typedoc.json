{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Powership",
  "entryPointStrategy": "packages",
  "entryPoints": [
    "packages/accounts",
    "packages/babel-plugins",
    "packages/powership",
    "packages/entity",
    "packages/utils",
    "packages/logstorm",
    "packages/mongo",
    "packages/schema",
    "packages/transporter"
  ],
  "exclude": ["**/*.test.ts"],
  "plugin": ["typedoc-plugin-markdown"],
  "out": "docs",
  "hideBreadcrumbs": false,
  "hideInPageTOC": false,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "entryDocument": "README.md"
}
